<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.administrator.myapplication">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" ></uses-permission>"
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE"></uses-permission>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE" />
    <!-- androdpn-end -->
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.single_layout.myapplication.EnchantingFlower"
            android:screenOrientation="landscape"
            android:label="妖艳花">
        </activity>

        <activity
            android:name="com.example.single_layout.myapplication.SmartCar"
            android:screenOrientation="landscape"
            android:label="智能小车">
        </activity>

        <activity
            android:name="com.example.single_layout.myapplication.WaterSprayTank"
            android:screenOrientation="landscape"
            android:label="喷水罐">
        </activity>

        <activity
            android:name="com.example.single_layout.myapplication.InfraredLaser"
            android:screenOrientation="landscape"
            android:label="红外激光">
        </activity>

        <activity
            android:name="com.example.single_layout.myapplication.ToyCar"
            android:screenOrientation="landscape"
            android:label="玩具车">
        </activity>

        <activity
            android:name="com.example.single_layout.myapplication.sack"
            android:screenOrientation="landscape"
            android:label="骰子机">
        </activity>

        <activity
            android:name="com.example.single_layout.myapplication.snake"
            android:screenOrientation="landscape"
            android:label="玩具蛇">
        </activity>

        <activity
            android:name="com.example.single_layout.myapplication.mouse"
            android:screenOrientation="landscape"
            android:label="玩具鼠">
        </activity>

        <activity
            android:name="com.example.single_layout.myapplication.FMoppingRobot"
            android:screenOrientation="landscape"
            android:label="扫地机器人">
        </activity>

        <activity
            android:name="com.example.single_layout.myapplication.text"
            android:screenOrientation="landscape"
            android:label="测试按钮">
        </activity>

        <activity
            android:name="com.example.single_layout.myapplication.SteeringEngine"
            android:screenOrientation="landscape"
            android:label="逗猫棒">
        </activity>

        <activity
            android:name="com.example.single_layout.myapplication.init_connet"
            android:screenOrientation="portrait"
            android:label="连接模式">
        </activity>



    </application>

</manifest>